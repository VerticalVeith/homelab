{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "argocd": {
    "managerFilePatterns": [
      "/manifests/.*\\.yaml/"
    ]
  },
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/manifests/apps/immich/helm-all.yaml"
      ],
      "matchStrings": [
        "tag:\\s*(?<currentValue>\\d+\\.\\d+\\.\\d+)"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "immich-app/immich",
      "versioningTemplate": "semver"
    }
  ]
}
